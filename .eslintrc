{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "linebreak-style": 0,
    "no-trailing-spaces": [2, { "skipBlankLines": true }],
    "operator-linebreak": "off",
    "no-debugger": "off",
    "max-len": ["error", { "code": 100 }]
  },
  "extends": ["eslint:recommended", "google"]
}